{
  "name": "vue-zorm",
  "version": "0.1.5",
  "packageManager": "pnpm@7.30.0",
  "description": "Type-safe form for Vue using Zod.",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/vue-zorm#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/vue-zorm.git"
  },
  "bugs": "https://github.com/wobsoriano/vue-zorm/issues",
  "keywords": [
    "form",
    "zod",
    "vue",
    "validation"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build-fast": "tsup src/index.ts --format cjs,esm --minify",
    "build": "pnpm run build-fast --dts-resolve",
    "dev": "vite serve dev",
    "dev:build": "vite build dev",
    "dev:preview": "vite preview dev",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.2.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.0",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^6.6.1",
    "@types/node": "^18.11.18",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.2.7",
    "bumpp": "^8.2.1",
    "eslint": "^8.30.0",
    "jsdom": "^21.0.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vitest": "^0.27.1",
    "vue": "^3.2.45",
    "zod": "^3.20.2"
  }
}
